project 'ImagePlayer/ImagePlayer.xcodeproj'

# Uncomment this line to define a global platform for your project
platform :ios, '10.0'

target 'ImagePlayer' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ImagePlayer

  # SuperDelegate provides a clean application delegate interface and protects you from bugs in the application lifecycle
  # https://github.com/square/SuperDelegate
  pod 'SuperDelegate', '~> 0.9.0'

  # The ultimate API for iOS & OS X Auto Layout â€” impressively simple, immensely powerful. Objective-C and Swift compatible.
  # https://github.com/PureLayout/PureLayout
  pod 'PureLayout', '~> 3.0.2'

  # Fresh look at work with gestures in Swift.
  # https://github.com/igormatyushkin014/Sensitive
  pod 'Sensitive'

  # Modern Swift API for NSUserDefaults
  # https://github.com/radex/SwiftyUserDefaults
  pod 'SwiftyUserDefaults', '~> 3.0.0'

  # A clean and lightweight progress HUD for your iOS and tvOS app.
  # https://github.com/SVProgressHUD/SVProgressHUD
  pod 'SVProgressHUD', '~> 2.1.2'

  # Closures on most used UIKit methods
  # https://github.com/Reflejo/LambdaKit
  pod 'LambdaKit'

  # A unified API to ask for permissions on iOS
  # https://github.com/delba/Permission
  pod 'Permission/Photos', '~> 2.0.3'

  # MIDI Library for Objective-C and Swift OS X and iOS apps.
  # https://github.com/mixedinkey-opensource/MIKMIDI
  pod 'MIKMIDI', '~> 1.6.1'
  
  # Fabulous Image Processing in Swift
  # https://github.com/gavinbunney/Toucan
  pod 'Toucan', '~> 0.6'
  
  # Swift audio synthesis, processing, & analysis platform for iOS, macOS and tvOS
  # http://audiokit.io
  pod 'AudioKit'

  target 'ImagePlayerTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ImagePlayerUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
